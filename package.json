{
  "name": "angular-cesium",
  "version": "0.0.10",
  "license": "MIT",
  "description": "Angular library for working with Cesium.",
  "repository": "https://github.com/TGFTech/angular-cesium",
  "keywords": [
    "Angular2",
    "Cesium",
    "Angular Cesium",
    "Angular2 Cesium"
  ],
  "homepage": "https://github.com/TGFTech/angular-cesium#readme",
  "main": "bundles/angular-cesium.umd.js",
  "typings": "src/main.d.ts",
  "scripts": {
    "clean_src": "rimraf src/*.d.ts src/components/**/*.d.ts src/models/**/*.d.ts src/pipes/**/*.d.ts src/services/**/*.d.ts src/utils/**/*.d.ts src/**/*.metadata.json src/**/*.js src/**/*.js.map src/**/*.ngfactory.ts src/**/*.ngstyle.ts src/**/*.ngsummary.json",
    "cleanup": "npm run clean_src && rimraf index.js index.js.map index.d.ts index.metadata.json build bundles",
    "start": "webpack-dev-server --inline --progress --port 8080",
    "lint": "tslint -c tslint.json --project tsconfig.aot.json",
    "test": "karma start",
    "ngc": "ngc -p tsconfig.aot.json",
    "aot": "webpack --config webpack.aot.config.js -p",
    "rollup": "rollup -cza",
    "bundle": "npm run cleanup && npm run lint && npm run ngc && npm run rollup",
    "build": "rimraf dist && webpack --config config/webpack.dev.js --progress --profile --bail",
    "test-server": "node demo/performance-server/server.js",
    "compile-docs": "rimraf docs && compodoc -p src/tsconfig.compodoc.json -d docs -n \"Angular 2 Cesium\"",
    "serve-docs": "compodoc -s -d ./docs -r 4200",
    "compileNserve-docs": "npm run compile-docs && npm run serve-docs"
  },
  "peerDependencies": {
    "@angular/core": "^2.4.8",
    "@angular/common": "^2.3.0"
  },
  "devDependencies": {
    "@angular/common": "~2.3.0",
    "@angular/compiler": "~2.3.0",
    "@angular/compiler-cli": "~2.3.0",
    "@angular/core": "~2.3.0",
    "@angular/forms": "~2.3.0",
    "@angular/http": "~2.3.0",
    "@angular/platform-browser": "~2.3.0",
    "@angular/platform-browser-dynamic": "~2.3.0",
    "@angular/router": "~3.3.0",
    "@types/core-js": "^0.9.35",
    "@types/jasmine": "2.5.36",
    "@types/node": "6.0.63",
    "@types/selenium-webdriver": "^2.53.33",
    "angular2-template-loader": "^0.4.0",
    "awesome-typescript-loader": "3.0.3",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.4.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.23.0",
    "babel-preset-env": "^1.2.1",
    "bower": "^1.7.9",
    "browserify": "^13.1.1",
    "cesium": "1.32.1",
    "codelyzer": "^3.0.1",
    "compodoc": "0.0.41",
    "concurrently": "^3.1.0",
    "copy-webpack-plugin": "^4.0.1",
    "core-js": "^2.4.1",
    "css-loader": "^0.23.1",
    "del": "^2.2.0",
    "es6-module-loader": "^0.17.11",
    "es6-shim": "^0.35.2",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "^0.8.5",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.26.0",
    "jasmine": "^2.4.1",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.22",
    "karma-coverage": "^1.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.0",
    "merge2": "^1.0.2",
    "null-loader": "^0.1.1",
    "phantomjs-prebuilt": "^2.1.7",
    "raw-loader": "^0.5.1",
    "reflect-metadata": "0.1.3",
    "rimraf": "^2.5.2",
    "rollup": "^0.41.6",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-uglify": "^1.0.1",
    "run-sequence": "^1.2.1",
    "rxjs": "5.0.0-beta.12",
    "socket.io": "^1.7.3",
    "style-loader": "^0.13.1",
    "ts-helpers": "^1.1.1",
    "ts-mockito": "1.1.4",
    "tslint": "4.4.2",
    "typescript": "2.0.10",
    "webpack": "2.1.0-beta.15",
    "webpack-dev-server": "1.16.3",
    "webpack-merge": "0.14.1",
    "webpack-node-externals": "^1.5.4",
    "zone.js": "^0.6.23"
  },
  "dependencies": {
    "angular2parse": "^1.0.4",
    "geodesy": "^1.1.1",
    "json-string-mapper": "^1.0.0",
    "primitive-primitives": "0.0.6"
  }
}
